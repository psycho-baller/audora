{
  "expo": {
    "name": "Audora",
    "slug": "audora",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "audora",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ramimaalouf.audora",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ramimaalouf.audora",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "audora"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsFileWithMaxCount": 10,
            "NSExtensionActivationSupportsText": true,
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
            "NSExtensionActivationSupportsImageWithMaxCount": 1,
            "NSExtensionActivationSupportsVideoWithMaxCount": 1,
            "NSExtensionActivationSupportsAudioWithMaxCount": 1
          },
          "androidIntentFilters": [
            "audio/*",
            "video/*",
            "text/*"
          ]
        }
      ],
      "expo-font"
      // "./plugins/with-share-extension-pods"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "78ec57bd-0204-429b-84c4-1d5b3fe8d6d6"
        // "build": {
        //   "experimental": {
        //     "ios": {
        //       "appExtensions": [
        //         {
        //           "targetName": "ShareExtension",
        //           "bundleIdentifier": "com.ramimaalouf.audora.share-extension",
        //           "entitlements": {
        //             "com.apple.security.application-groups": [
        //               "group.com.ramimaalouf.audora"
        //             ]
        //           }
        //         }
        //       ]
        //     }
        //   }
        // }
      }
    }
  }
}